{
  "name": "react-risu-test1",
  "version": "0.1.0",
  "description": "React Risu Test1 for RISU AI",
  "main": "src/index.jsx",
  "browser": "dist/react_risu_test1.js",
  "unpkg": "dist/react_risu_test1.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "risu-ai",
    "cdn",
    "plugin",
    "react"
  ],
  "author": "Chaebboong",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:server\" \"npm run dev:vite\"",
    "dev:server": "node scripts/dev-server.js",
    "dev:vite": "cross-env NODE_ENV=development vite build --watch",
    "build": "vite build",
    "build:dev": "cross-env NODE_ENV=development vite build",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css}\"",
    "release": "node scripts/release.js",
    "release:minor": "node scripts/release.js -- minor",
    "release:patch": "node scripts/release.js -- patch",
    "release:major": "node scripts/release.js -- major"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "prettier": "^3.2.5",
    "terser": "^5.36.0",
    "vite": "^5.0.0",
    "vite-plugin-css-injected-by-js": "^3.5.0",
    "ws": "^8.18.3"
  },
  "dependencies": {
    "idb": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
